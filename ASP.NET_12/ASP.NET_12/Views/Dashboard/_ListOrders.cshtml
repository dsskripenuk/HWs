@model IEnumerable<ASP.NET_12.Models.OrderViewModel>

<h2>Orders:</h2>
<table class="table">
    <tr>
        <th>Full Name Customer</th>
        <th>Address</th>
        <th>Product</th>
        <th>Count product</th>
        <th>Total price</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.FullName</td>
            <td>@item.Address</td>
            <td>@item.NameProduct</td>
            <td>@item.CountProduct</td>
            <td>@item.Price</td>
        </tr>
    }

</table>
